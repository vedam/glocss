# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{json,yml,toml}]
indent_style = space
indent_size = 2

[*.{css,md}]
trim_trailing_whitespace = false

[*.{md}]
indent_style = tab
